    "LinkedListSingle": {
        "prefix": "!linkedlist",
        "body": [
            "class ListNode:",
            "\tdef __init__(self, val=${2:0}, next=None):",
            "\t\tself.val = val",
            "\t\tself.next = next\n",
            "def makeList(values: List[${1:int}]) -> ListNode | None:",
            "\tif not values:",
            "\t\treturn",
            "\thead = ListNode(values[0])",
            "\tprev = head",
            "\tfor i in range(1, len(values)):",
            "\t\tcurr = ListNode(values[i])",
            "\t\tprev.next = curr",
            "\t\tprev = curr",
            "\treturn head\n",
            "def toString(head: ListNode | None) -> str:",
            "\tvalues = []",
            "\tcurr = head",
            "\twhile curr != None:",
            "\t\tvalues.append(str(curr.val))",
            "\t\tcurr = curr.next",
            "\treturn f\"[{' -> '.join(values)}]\""
        ],
        "description": "A singly-linked list with various free standing utility functions"
    },
