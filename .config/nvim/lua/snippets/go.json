    "Stack": {
        "prefix": "Stack",
        "body": [
            "type ${1:Stack} []${2:int}\n",
            "func (s $1) Push(val $2) $1 {",
            "\treturn append(s, val)",
            "}\n",
            "func (s $1) Pop() ($1, $2) {",
            "\tsz := len(s)",
            "\tif sz == 0 {",
            "\t\tpanic(\"Just tried to Pop() from an empty $1.\")",
            "\t}",
            "\treturn s[:sz-1], s[sz-1]",
            "}\n",
            "// Usage:",
            "// var s $1",
            "// OR s := make($1, 0)",
            "// s = s.Push(1) // note: this example uses int stack",
            "// var popped int",
